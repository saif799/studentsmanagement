{
  "expo": {
    "name": "Students_Management",
    "slug": "studentsmanagement",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Students_Management",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saiiiforg.studentsmanagement"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saiiiforg.studentsmanagement"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b6075b4f-811e-4c89-a1de-6ed48b3ba319"
      }
    },
    "owner": "sa_iiif-org",
    "updates": {
      "url": "https://u.expo.dev/b6075b4f-811e-4c89-a1de-6ed48b3ba319"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "expo": {
      "assetBundlePatterns": ["/assets/images/icon.png"]
    }
  }
}
